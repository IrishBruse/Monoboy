{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Monoboy.Desktop",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build Monoboy.Desktop",
            "program": "${workspaceFolder}/Monoboy.Desktop/bin/Debug/net6.0/Monoboy.Desktop.dll",
            "cwd": "${workspaceFolder}/Monoboy.Desktop",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "Monoboy.Frontend",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build Monoboy.Frontend",
            "program": "${workspaceFolder}/Monoboy.Frontend/bin/Debug/net6.0/Monoboy.Frontend.dll",
            "cwd": "${workspaceFolder}/Monoboy.Frontend",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}
